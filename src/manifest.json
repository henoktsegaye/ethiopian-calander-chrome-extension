{
  "manifest_version": 3,
  "name": "Ethiopian Calender Date",
  "description": "A chrome extension that shows current Ethiopian date",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },

  "icons": {
    "128": "icon-128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["icon-128.png", "icon-34.png"],
      "matches": []
    }
  ]
}
